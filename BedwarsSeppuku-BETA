local SeppukuHub = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = SeppukuHub:MakeWindow({Name = "Seppuku | Game Mega | ", HidePremium = false, SaveConfig = true, ConfigFolder = "SeppukuBETA"})

--notification
SeppukuHub:MakeNotification({
	Name = "Seppuku",
	Content = "SeppukuHub | Bedwars | Loaded!",
	Image = "rbxassetid://4483345998",
	Time = 5
})






local Combat = Window:MakeTab({
	Name = "Combat",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})


local Player = Window:MakeTab({
	Name = "Player",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})


local Visual = Window:MakeTab({
	Name = "Visual",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})


local Scripts = Window:MakeTab({
	Name = "Scripts",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})


local Other = Window:MakeTab({
	Name = "Other",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})



--combat
Combat:AddToggle({
	Name = "Killaura",
	Default = false,
	Callback = function(Value)
		print(Value)
	end    
})


Combat:AddToggle({
	Name = "Multiaura",
	Default = false,
	Callback = function(Value)
		print(Value)
	end    
})


Combat:AddSlider({
	Name = "Reach",
	Min = 0,
	Max = 18,
	Default = 14,
	Color = Color3.fromRGB(196, 40, 28),
	Increment = 1,
	ValueName = "ReachDistance",
	Callback = function(Value)
		print(Value)
	end    
})


Combat:AddButton({
	Name = "NoClickDelay (Mine)",
	Callback = function()
      		print("button pressed")
  	end    
})


Combat:AddButton({
	Name = "NoClickDelay (Rektsky)",
	Callback = function()
      		print("button pressed")
  	end    
})




--player
Player:AddSlider({
	Name = "Speed",
	Min = 0,
	Max = 23,
	Default = 20,
	Color = Color3.fromRGB(196, 40, 28),
	Increment = 1,
	ValueName = "PlayerSpeed",
	Callback = function(Value)
		print(Value)
	end    
})


Player:AddToggle({
	Name = "Sprint",
	Default = false,
	Callback = function(Value)
		workspace.Gravity = 0
	end    
})


Player:AddToggle({
	Name = "Spider",
	Default = false,
	Callback = function(Value)
workspace.Gravity = 192
	end    
})


Player:AddBind({
	Name = "GravityFly",
	Default = Enum.KeyCode.R,
	Hold = false,
	Callback = function()
 if callback then
workspace.Gravity = 0
else
workpsace.Gravity = 192
end
end   
})


Player:AddBind({
	Name = "LongJump",
	Default = Enum.KeyCode.Z,
	Hold = false,
	Callback = function()
		print("press")
	end    
})


Player:AddBind({
	Name = "HighJump",
	Default = Enum.KeyCode.Q,
	Hold = false,
	Callback = function()
		print("press")
	end    
})




