Player:AddBind({
	Name = "Fly (hold)",
	Default = Enum.KeyCode.R,
	Hold = true,
	Callback = function(callback)
 if callback then
 workspace.Gravity = 1
else
 workspace.Gravity = 192
end 
end 
})


